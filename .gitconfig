[user]
	name = Yadong Wen
	email = ralph.wen@gmail.com
[credential]
    helper = cache --timeout=3600
[core]
    editor = vim
    pager = less -r
[color]
    ui = true
[color "branch"]
    current = yellow bold
    local = green bold
    remote = cyan bold
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = green bold
    changed = yellow bold
    untracked = red bold
[github]
    user = yadongwen
[alias]
    s = status
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
